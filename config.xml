<?xml version='1.0' encoding='utf-8'?>
<widget id="br.com.optiapps.line_it_medico" ios-CFBundleIdentifier="br.com.optiapps.lineitmedico" version="1.0.11" xmlns="http://www.w3.org/ns/widgets" xmlns:gap="http://phonegap.com/ns/1.0">
    <name>Line-it</name>
    <description>
    Acolhimento e acompanhemento
  </description>
    <author email="optiappsbrasil@gmail.com" href="https://www.opti-apps.com/#/">
